+nocsrf
PUT           /campaigns                      controllers.DatabaseController.createCampaignsTable
+nocsrf
POST          /campaigns                      controllers.DatabaseController.addCampaign
+nocsrf
DELETE        /campaigns/:resourceName        controllers.DatabaseController.deleteCampaign(resourceName:String)
