# ----- MainController -----
GET           /                               controllers.MainController.index
GET           /auth                           controllers.MainController.auth
GET           /oauth2callback                 controllers.MainController.oAuth2Callback
GET           /authuserinfo                   controllers.MainController.authUserData
+nocsrf
POST          /upload                         controllers.MainController.upload
+nocsrf
POST          /uploadtest                     controllers.MainController.createCampaigns
+nocsrf
DELETE        /auth                           controllers.MainController.deleteSession
# ----- DatabaseController -----
# Users
GET           /users                          controllers.DatabaseController.getUsers
GET           /users/:id                      controllers.DatabaseController.getUser(id:String)
+nocsrf
PUT           /users                          controllers.DatabaseController.createUsersTable
+nocsrf
POST          /users                          controllers.DatabaseController.addUser
+nocsrf
DELETE        /users/:id                      controllers.DatabaseController.deleteUser(id:String)
+nocsrf
PATCH         /users                          controllers.DatabaseController.updateUser
# Campaigns
